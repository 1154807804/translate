/**index.wxss**/
page{
  width: 100%;
  height: 100%;
}
#warp{
  overflow: hidden;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}
#logo{
  width: 200rpx;      
  height: 200rpx;     
  margin: 200rpx auto;
}
#logo image{          
  display: block;     
  width: 100%;        
  height: 100%;       
}
#text{
  width: 450rpx;            /*宽度设为450rpx*/
  height: 80rpx;            /*宽度设为80rpx*/
  margin: 0 auto;           /*让这个内容在页面居中*/
  font-size: 28rpx;         /*字体大小设为28rpx*/
  padding: 0 30rpx;         /*左右内填充20rpx*/
  background: #01aaaf;         /*背景颜色设为红色(#f00)*/
  border-radius: 100rpx;    /*边框圆角弧度为100rpx*/
  line-height: 80rpx;       /*行高设为80rpx  用于文字垂直居中*/
  color: #fff;              /*文字颜色为白色（#fff）  */
}
#text image{
  display: block;
  width: 45rpx;
  height: 45rpx;
  margin: 17.5rpx 0;
  float: right;
}